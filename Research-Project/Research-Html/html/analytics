  $(document).on("click", ".faculty-view-resume-icon", function (e) {
    $("#view-resume-modal").modal("toggle");
    let tr = $(this).closest("tr");
    let id = tr.data("userlid");
    $.ajax({
      url: "/vf/get-resume-by-user?user_id=296",
      type: "POST",
      success: function (response) {
        let data = JSON.parse(response.value);

        console.log("data received::Sidebar:::::::::::>>", "ROLE_ADMIN");

        if (data.resume_details != null) {
          let tableToAppend = `
            <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">

                <table class="table table-bordered">
                <thead>
                <tr>
                    <th>Resume Name</th>
                    <th>Resume Description</th>
                    <th>Actions</th>
                </tr>
                </thead>
                <tbody>`;
          for (let obj of data.resume_details) {
            tableToAppend += `
                <tr>
                    <td>${obj.name}</td>
                    <td class="user_id">${obj.description}</td>
                    <td>
                        <a href="/vf/view-resume?resume_lid=${obj.id}" class="application-preview" style="border:none; outline:none; text-decoration: none" >
                            <svg xmlns="http://www.w3.org/2000/svg" style="max-width:25px; max-height:30px;" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M2.036 12.322a1.012 1.012 0 010-.639C3.423 7.51 7.36 4.5 12 4.5c4.638 0 8.573 3.007 9.963 7.178.07.207.07.431 0 .639C20.577 16.49 16.64 19.5 12 19.5c-4.638 0-8.573-3.007-9.963-7.178z" />
                                <path stroke-linecap="round" stroke-linejoin="round" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z" />
                            </svg>
                        </a>`

                if("ROLE_ADMIN" == "ROLE_ADMIN" || "ROLE_ADMIN" == "User"){
                    tableToAppend +=  `<a href="/vf/resume?resume_lid=${obj.id}" class="application-preview" style="border:none; outline:none; text-decoration: none" >
                                            <svg xmlns="http://www.w3.org/2000/svg" style="max-width:25px; max-height:30px;" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                                                <path stroke-linecap="round" stroke-linejoin="round" d="M16.862 4.487l1.687-1.688a1.875 1.875 0 112.652 2.652L10.582 16.07a4.5 4.5 0 01-1.897 1.13L6 18l.8-2.685a4.5 4.5 0 011.13-1.897l8.932-8.931zm0 0L19.5 7.125M18 14v4.75A2.25 2.25 0 0115.75 21H5.25A2.25 2.25 0 013 18.75V8.25A2.25 2.25 0 015.25 6H10" />
                                            </svg>
                                        </a>`
                }

            tableToAppend +=       `</td>
                                </tr>`;
          }

          tableToAppend += `  </tbody>
                         </table>
                    </div>
       `;
          document.querySelector(".modal2-body").innerHTML = "";
          $(".modal2-body").html(tableToAppend);
        } else {
          $(".modal2-body").html('<h4 class="text-center">No Data Found </h4>');
        }
      },
      error: function (error) {
        console.log("error", error);
      },
    });
  });

  $(document).on("click", ".modal2-cancel-button", function () {
    $("#view-resume-modal").modal("toggle");
  });
</script>


